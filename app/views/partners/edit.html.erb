<h1>Editing partner</h1>

<% form_for(@partner) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :fp7_Number %><br />
    <%= f.text_field :fp7_Number %>
  </p>
  <p>
    <%= f.label :Name %><br />
    <%= f.text_field :Name %>
  </p>
  <p>
    <%= f.label :State %><br />
    <%= f.text_field :State %>
  </p>
  <p>
    <%= f.label :Address %><br />
    <%= f.text_area :Address %>
  </p>
  <p>
    <%= f.label :Phone %><br />
    <%= f.text_field :Phone %>
  </p>
  <p>
    <%= f.label :Email %><br />
    <%= f.text_field :Email %>
  </p>
  <p>
    <%= f.label :Site %><br />
    <%= f.text_field :Site %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @partner %> |
