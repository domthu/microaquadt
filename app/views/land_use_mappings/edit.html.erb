<h1>Editing land_use_mapping</h1>

<% form_for(@land_use_mapping) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @land_use_mapping %> |
<%= link_to 'Back to list', land_use_mappings_path %>

