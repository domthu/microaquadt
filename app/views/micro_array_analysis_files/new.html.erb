<h1>New micro array analysis file</h1>

<% form_for(@micro_array_analysis_file) do |f| %>
  <%= f.error_messages %>

  <div class="field select">
    <%= f.label :microarray %><br />
    <%= select :micro_array_analysis_file,:microarray_id, MicroArray.find(:all).collect{|p| [p.verbose_me, p.id]}%>
  </div>

  <div class="field txt">
    <%= f.label :MIANE_Standard, "MIANE Standard" %><br />
    <%= f.text_field :MIANE_Standard %>
  </div>

  <div class="field area">
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </div>

  <div class="actions">
    <%= f.submit 'Create' %>
  </div>

<% end %>

<%= link_to 'Back', micro_array_analysis_files_path %>

