<h1>Editing altitude type</h1>

<% form_for(@altitude_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @altitude_type %> |
<%= link_to 'Back', altitude_types_path %>
