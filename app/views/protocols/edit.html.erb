<h1>Editing protocol</h1>

<% form_for(@protocol) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :GrowthProtocol %><br />
    <%= f.text_area :GrowthProtocol %>
  </p>
  <p>
    <%= f.label :TreatmentProtocol %><br />
    <%= f.text_area :TreatmentProtocol %>
  </p>
  <p>
    <%= f.label :ExtractProtocol %><br />
    <%= f.text_area :ExtractProtocol %>
  </p>
  <p>
    <%= f.label :LabelProtocol %><br />
    <%= f.text_area :LabelProtocol %>
  </p>
  <p>
    <%= f.label :HybProtocol %><br />
    <%= f.text_area :HybProtocol %>
  </p>
  <p>
    <%= f.label :ScanProtocol %><br />
    <%= f.text_area :ScanProtocol %>
  </p>
  <p>
    <%= f.label :DataProcessing %><br />
    <%= f.text_area :DataProcessing %>
  </p>
  <p>
    <%= f.label :ValueDefinition %><br />
    <%= f.text_area :ValueDefinition %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @protocol %> |
<%= link_to 'Back', protocols_path %>

